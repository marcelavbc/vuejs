sContent":["<template>\n  <transition appear @before-enter=\"beforeEnter\" @enter=\"enter\" :css=\"false\">\n    <slot />\n  </transition>\n</template>\n\n<script>\nexport default {\n  props: {\n    duration: {\n      type: Number,\n      default: 500,\n    },\n    delay: {\n      type: Number,\n      default: 100,\n    },\n    translate: Array,\n  },\n  methods: {\n    beforeEnter(el) {\n      el.style.opacity = 0;\n      if (this.translate)\n        el.style.transform = \"translate(\" + this.translate.join(\",\") + \")\";\n    },\n    enter(el) {\n      let transitions =\n        \"opacity \" + this.duration + \"ms ease-in-out \" + this.delay + \"ms\";\n      if (this.translate)\n        transitions +=\n          \", transform \" +\n          this.duration +\n          \"ms ease-in-out \" +\n          this.delay +\n          \"ms\";\n      el.style.transition = transitions;\n      getComputedStyle(el);\n      setTimeout(() => {\n        el.style.opacity = 1;\n        if (this.translate) el.style.transform = \"initial\";\n      });\n    },\n  },\n};\n</script>"],"sourceRoot":"src/components/utils"}]}